{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "include": ["./lib", "./server"],
    "exclude": ["node_modules", "test"]
  },
  "opts": {
    "destination": "./docs",
    "recurse": true,
    "readme": "./README.md"
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true,
    "default": {
      "includeDate": false,
      "outputSourceFiles": true
    }
  }
}
